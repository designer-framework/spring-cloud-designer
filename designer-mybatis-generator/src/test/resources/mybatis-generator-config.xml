<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- 配置生成器 -->
<generatorConfiguration>

    <context id="mysql" defaultModelType="flat"
             targetRuntime="org.designer.mybatis.ext.IntrospectedTableMyBatis3DynamicSqlImplV3"
             introspectedColumnImpl="org.mybatis.generator.api.IntrospectedColumn">

        <!-- 模块名 -->
        <property name="modelName" value="student"/>

        <!-- 实体类 父类路径 -->
        <property name="modelSuperClass" value="org.designer.mybatis.base.entity.BaseEntity"/>

        <!-- 实体类 项目路径 -->
        <property name="modelProject" value="designer-mybatis-generator/src/test/java"/>
        <!-- 实体类 包路径 -->
        <property name="javaModelPackage" value="org.designer.%s.model"/>

        <!-- Service 项目路径 -->
        <property name="serviceProject" value="designer-mybatis-generator/src/test/java"/>
        <!-- Service 包路径 -->
        <property name="javaServicePackage" value="org.designer.%s.service"/>

        <!-- ServiceImpl 项目路径 -->
        <property name="serviceImplProject" value="designer-mybatis-generator/src/test/java"/>
        <!-- ServiceImpl 包路径 -->
        <property name="javaServiceImplPackage" value="org.designer.%s.service.impl"/>

        <property name="clientProject" value="designer-mybatis-generator/src/test/java"/>
        <property name="javaClientPackage" value="org.designer.%s.mapper"/>

        <!-- XML的Interface路径 -->
        <property name="javaMapperProject" value="designer-mybatis-generator/src/test/java"/>
        <!-- XML的Resource路径 -->
        <property name="xmlMapperPackage" value="mapper.%s"/>

        <!-- 暂时没用 -->
        <property name="sqlMapPackage" value="mybatis.%s"/>

        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
        <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <plugin type="org.designer.mybatis.plugins.model.SwaggerAnnotationPlugin"/>
        <plugin type="org.designer.mybatis.plugins.mapper.BaseMapperPlugin"/>

        <commentGenerator type="org.mybatis.generator.internal.DefaultCommentGenerator">
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="false"/>
            <property name="addRemarkComments" value="true"/>
            <property name="dateFormat" value="yyyy-MM-dd HH:mm:ss"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.186.128:3306/xxl_job?useUnicode=true&amp;characterEncoding=utf8&amp;useSSL=false&amp;serverTimezone=Asia/Shanghai"
                        userId="root"
                        password="Onlyyouroo@@@">
        </jdbcConnection>

        <javaTypeResolver type="org.mybatis.generator.internal.types.JavaTypeResolverDefaultImpl">
            <property name="forceBigDecimals" value="true"/>
            <property name="useJSR310Types" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="org.designer.mybatis.model"
                            targetProject="designer-mybatis-generator/src/test/java">
            <property name="constructorBased" value="false"/>
            <property name="enableSubPackages" value="true"/>
            <property name="immutable" value="false"/>
            <property name="rootClass" value="org.designer.mybatis.base.entity.BaseEntity"/>
            <property name="trimStrings" value="false"/>
        </javaModelGenerator>

        <!--xml-->
        <sqlMapGenerator targetPackage="org.designer.mybatis"
                         targetProject="designer-mybatis-generator/src/test/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <javaClientGenerator targetPackage="org.designer.mybatis.mapper"
                             targetProject="designer-mybatis-generator/src/test/java"
                             type="">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="xxl_job_log">
            <property name="constructorBased" value="false"/>
            <property name="ignoreQualifiersAtRuntime" value="false"/>
            <property name="immutable" value="false"/>
            <property name="modelOnly" value="false"/>
            <property name="useActualColumnNames" value="false"/>
            <columnOverride column="job_group" property="jobGroup">
                <property name="property" value="job_group"/>
            </columnOverride>
        </table>
    </context>

</generatorConfiguration>